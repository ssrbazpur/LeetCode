//Time:O(n^2)

class Solution {
public:
    string removeKdigits(string num, int k) {
      string ans="";

        for(char &c:num)
        {
            while(ans.size() && ans[ans.size()-1]>c && k)
            {
                cout<<ans[ans.size()-1];
                ans.pop_back();
                k--;
            }
            if(ans.size()||c!='0')ans.push_back(c);
        }
        for(char a:ans){
            cout<<a;
        }
        while(ans.size() && k--)         
        {
            ans.pop_back();
        }
        return (ans=="")?"0":ans;
        
    }
};
